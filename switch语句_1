//switch表达式
#include<stdio.h>
int main(void)
{
	int day = 0;
	scanf("%d", &day);
	switch (day)//括号内必须是 整型 常量 表达式
	{
	case 1:
		printf("Monday\n");
		break;
	case 2:
		printf("Tuesday\n");
		break;
	case 3:
		printf("Wednesday\n");
		break;
	case 4:
		printf("Thursday\n");
		break;
	case 5:
		printf("Friday\n");
		break;
	case 6:
		printf("Saturday\n");
		break;
	case 7:
		printf("Sunday\n");
		break;
	default:
		printf("非法输入\n");
		break;
	}
	return 0;
}
