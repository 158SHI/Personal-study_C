//条件操作符
#include<stdio.h>
int main(void)
//{
//	int a = 0;
//	int b = 0;
//	scanf("%d", &a);
//	b = (a > 5 ? 3 : -3);
//	printf("b=%d\n", b);
//}
{
	int a = 10;
	int b = 20;
	int ret = 0;
	ret = (a > b ? a : b);//输出两个数中的最大值
	printf("ret=%d\n", ret);
}

//条件操作符
int get_max(int a, int b)
{
	return (a > b ? a : b);//条件操作符
}

#include<stdio.h>
int main(void)
{
	int a = 10;
	int b = 20;
	int max = get_max(a, b);//函数引用操作符 （  ）
	printf("max=%d", max);
	return 0;
}
